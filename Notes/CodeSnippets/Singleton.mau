		#region Singleton
			private static volatile CLASS instance=new CLASS();
			private static object syncRoot=new Object();
			//constructor singleton multithread safe
			static CLASS ( ) { }
			public static CLASS Instance {
				get {
					if( instance==null ) {
						lock( syncRoot ) {
							instance=new CLASS();
						}
					}
					return instance;
				}
			}
		#endregion
